{
    "name": "meebio/omnipay-secure-trading",
    "description": "Secure Trading driver for the Omnipay PHP payment processing library",
    "keywords": [
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "purchase",
        "secure trading",
        "securetrading"
    ],
    "homepage": "https://github.com/meebio/omnipay-secure-trading",
    "license": "MIT",
    "authors": [
        {
            "name": "John Jablonski",
            "email": "jablonski.kce@gmail.com",
            "homepage": "http://jablonski.me",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.2",
        "omnipay/common": "~3.0"
    },
    "require-dev": {
        "omnipay/tests": "~3.0",
        "phpunit/phpunit": "6.5.9"
    },
    "autoload": {
        "psr-4": {
            "Omnipay\\SecureTrading\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\SecureTrading\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
